{
  "name": "cl-teachable-machine-boilerplate",
  "version": "0.1.0",
  "license": "",
  "dependencies": {
    "deeplearn": "~0.5.0",
    "deeplearn-knn-image-classifier": "^0.3.0",
    "pre-commit": "^1.2.2"
  },
  "scripts": {
    "http": "budo deeplearn/main.js:dist/build.js --live --host ::",
    "https": "budo deeplearn/main.js:dist/build.js --ssl --cors --live --host ::",
    "build": "browserify deeplearn/main.js -o dist/deeplearn.js"
  },
  "pre-commit": [
    "build"
  ],
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ],
          "plugins": [
            "syntax-async-functions",
            "transform-regenerator"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0",
    "budo": "^11.2.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-uglify": "^3.0.0"
  }
}
